{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "max-len": ["error", { "code": 135 }],
      "no-console": "off"
    }
}
